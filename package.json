{
  "name": "express-object-router",
  "version": "1.3.0",
  "description": "This library facilitate describing endpoints for REST API servers. The main feature of this library is determining method, path, controller, middlewares, and validation of query object, url object and body object with one object.",
  "scripts": {
    "lint": "eslint lib/**/*.js test/**/*.js examples/**/*.js index.js methods.js symbols.js",
    "unit-test": "mocha test/*.test.js",
    "test": "npm run lint && npm run unit-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArturAralin/express-object-router.git"
  },
  "keywords": [
    "express",
    "router",
    "promise",
    "rest",
    "api",
    "restful"
  ],
  "author": "Artur A.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArturAralin/express-object-router/issues"
  },
  "homepage": "https://github.com/ArturAralin/express-object-router#readme",
  "devDependencies": {
    "api-doc": "^4.0.0",
    "apidoc": "^0.17.6",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "express": "4.x",
    "mocha": "^4.0.1",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "express-promise-router": "^2.0.0",
    "glob": "^7.1.2",
    "joi": "^13.0.1",
    "ramda": "^0.25.0",
    "readline-sync": "^1.4.9",
    "commander": "^2.15.1"
  },
  "engines": {
    "node": ">= 6.11.5"
  },
  "peerDependencies": {
    "express": "^4.x",
    "joi": "^13.x"
  },
  "preferGlobal": true,
  "bin": {
    "eo-router": "./cli/index.js"
  }
}
